class Point {
    say_hi() { print( "hi!" ); }
    say_this_X() { print( this.X ); }
}

var p = Point();
print( p ); // <-- p = delegate for Point constructor

p.X = 5; // <-- setter
p.Y = 10;

print( p.X ); // <-- getters
print( p.Y );

p.say_hi(); // <-- method call
p.say_this_X(); // <-- method call that uses 'this'
